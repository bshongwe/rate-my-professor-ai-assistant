# MongoDB connection string (edit this for MongoDB)
MONGODB_URI=mongodb+srv://username:password@cluster0.mongodb.net/dbname?retryWrites=true&w=majority

# JWT secret key (edit this for JWTs)
JWT_SECRET=your_jwt_secret_key

# Pinecone API key and environment (edit this for Pinecone)
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment

# Firebase credentials (edit this for Firebase)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
FIREBASE_APP_ID=your_firebase_app_id

# Stripe API key (edit this for Stripe)
STRIPE_API_KEY=your_stripe_api_key

# Next.js secret for JWT (edit this for Next+JWT)
NEXTAUTH_SECRET=your_nextauth_secret_key

# API URL (for deployment purposes)
NEXT_PUBLIC_API_URL=https://<url>

